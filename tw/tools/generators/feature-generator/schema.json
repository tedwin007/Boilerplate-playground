{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "feature-generator",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "Library name",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "singleItemName": {
    "type": "string",
    "description": "single item name",
    "$default": {
      "$source": "argv",
      "index": 1
    }
  },
  "required": ["name"]
}
